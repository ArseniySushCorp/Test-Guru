<li class="collection-item">
  <div class="collection-cell">
    <a href=<%=question_path(question) %>><%= question.text %></a>
  </div>

  <div class="collection-cell">
    <a href=<%=edit_question_path(question) %> class="secondary-content">
      <i class="material-icons blue-text">mode_edit</i>
    </a>

    <a href=<%=question_path(question) %> data-method="delete" class="secondary-content">
      <i class="material-icons blue-text">delete</i>
    </a>
  </div>
</li>