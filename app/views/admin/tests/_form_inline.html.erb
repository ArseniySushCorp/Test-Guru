<%= form_with model: [:admin, test], url: update_inline_admin_test_path(test), 
              local: true,
              class: 'form-inline hide',
              data: {test_id: test.id } do |f| 
%>
  <div class="input-field inline">
    <%= f.text_field :title %>
  </div>
  <%= f.submit t('.submit'), class: 'btn blue' %>

  <%= link_to "Cancel", admin_tests_path, 
      class: "form-inline__cancel-btn btn blue", 
      data: {test_id: test.id } %>
<% end %>
