<li class="collection-item">
  <div class="collection-cell">
    <%= link_to test.title, test_path(test) %>

    <span>level: <%= test.level %></span>

    <span>category: <%= test.category.title %></span>

  </div>

  <div class="collection-cell">
    <%= button_to 'Start', start_test_path(test), class: 'btn blue btn-start' %>
    <%= link_to "Edit", edit_test_path(test), method: :delete, class: 'secondary-content btn blue'%>
    |
    <%= link_to "Delete", test_path(test), method: :delete, class: 'secondary-content btn blue'%>
  </div>
</li>